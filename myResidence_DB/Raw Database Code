//create the database
create database myResidence_DB;

//create a table for the residences
CREATE TABLE residence (
    residence_ID		  INTEGER NOT NULL,
    living_style		  VARCHAR(255)	CHECK(living_style = "Residence Hall" or living_style = "Apartment" or living_style = "Townhouse"),
    room_type			    VARCHAR(225)	CHECK(room_type = "Double" or room_type = "Basic Single" or room_type = "Single"),
    residence_price		INTEGER NOT NULL,
    residence_address	VARCHAR(255),
    has_mealplan		  BOOLEAN,
    student_usernames	VARCHAR(255),
    PRIMARY KEY (residence_ID)
    );

//create a table for the students
CREATE TABLE student (
    student_username	CHAR(8)	NOT NULL,
    student_password	VARCHAR(255) NOT NULL,
    student_email		  CHAR(21) NOT NULL,
    student_name		  VARCHAR(255) NOT NULL,
    year_num			    INTEGER NOT NULL,
    residence_ID		  INTEGER,
    application_pending		BOOLEAN,
    PRIMARY KEY (student_username),
    FOREIGN KEY (residence_ID) REFERENCES residence (residence_ID)
		ON UPDATE CASCADE ON DELETE SET NULL
    );

//create a table for the managers
CREATE TABLE manager (
    manager_username		CHAR(8)	NOT NULL,
    manager_password		VARCHAR(255) NOT NULL,
    manager_email			  CHAR(21) NOT NULL,
    manager_name			  VARCHAR(255) NOT NULL,
    PRIMARY KEY (manager_username)
    );

